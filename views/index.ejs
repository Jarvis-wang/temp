

<div class="container-fluid">
  <div class=row>
    <div class="col col-md-12">
      <div style="width:100%;">
       
       
        
        
        </div>











     <div class="lay" style="background:#F67280;">

      <p>There are
        <%= categorys.query.length %> categorys.
          <br/>
      </p>
      <% for (var i = categorys.query.length-1; i >= 0; i--) {   
        category = categorys.query[i];   %>
        <div class="panel panel-default">
          <div class="panel-heading"></div>
          <div class="text-muted pull-right"></div>
          <div class="panel-body">
            ID:
            <%=  category._id %>
              <br> Email:
              <%=category.email%>
                <br/> Given Name:
                <%=category.given%>
                  <br/> Family Name:
                  <%=category.family%>
                  <br/> Address:
                  <br/>Street 1:
                  <%=category.street1%>
                  </br>Street 2:
                  <%=category.street2%>
                </br>City:
                <%=category.city%>
              </br>State:
              <%=category.state%>
            </br>Zip:
            <%=category.zip%>
            <br/>
            <%=category.country%>

      
          </div>
        </div>
        <% } %>
<center><a href="/category" class="btn btn-info">Open category Data</a></center>


     </div>
     <div class="lay" style="background:#C06C84;">


      <p>There are
        <%= orders.query.length %> orders.
          <br/>
      </p>
      <% for (var i = orders.query.length-1; i >= 0; i--) {   
        order = orders.query[i];   %>
        <div class="panel panel-default">
          <div class="panel-heading"></div>
          <div class="text-muted pull-right"></div>
          <div class="panel-body">
            ID:
            <%=  order._id %>
              <br> Email:
              <%=order.email%>
                <br/> datePlaced:
                <%=order.datePlaced%>
                  <br/> dateShipped:
                  <%=order.dateShipped%>
                  <br/> paymentType:
                  <%=order.paymentType%>
                  </br>amountDue:
                  <%=order.amountDue%>
                </br>paid:
                <%=order.paid%>

          </div>
        </div>
        <% } %>

        <center><a href="/order" class="btn btn-info">Open Orders Data</a></center>
     </div>
     <div class="lay" style="background:#6C5B7B;">

      <p>There are
        <%= products.query.length %> products.
          <br/>
      </p>
      <% for (var i = products.query.length-1; i >= 0; i--) {   
        product = products.query[i];   %>
        <div class="panel panel-default">
          <div class="panel-heading"></div>
          <div class="text-muted pull-right"></div>
          <div class="panel-body">
            ID:
            <%=  product._id %>
              <br> productKey:
              <%=product.productKey%>
                <br/> description:
                <%=product.description%>
                  <br/> unitPrice:
                  <%=product.unitPrice%>
                 
          </div>
        </div>
        <% } %>

        <center><a href="/product" class="btn btn-info">Open Products Data</a></center>

     </div>
     <div class="lay" style="background:#355C7D;">

      <p>There are
        <%= orderLineItems.query.length %> orderLineItems.
          <br/>
      </p>
      <% for (var i = orderLineItems.query.length-1; i >= 0; i--) {   
        orderLineItem = orderLineItems.query[i];   %>
        <div class="panel panel-default">
          <div class="panel-heading"></div>
          <div class="text-muted pull-right"></div>
          <div class="panel-body">
            ID:
<%=  orderLineItem._id %>
  <br> orderID:
  <%=orderLineItem.orderID%>
    <br/> lineNumber:
    <%=orderLineItem.lineNumber%>
      <br/> productKey:
      <%=orderLineItem.productKey%>
      <br/> quantity:
      <%=orderLineItem.quantity%>
      
          </div>
        </div>
        <% } %>

        <center><a href="/orderLineItem" class="btn btn-info">Open orderLineItems Data</a></center>


     </div>
<style>
body{background:#F8B195;}
.lay{width:45%;height:auto;border:1px solid;float:left;margin-right: 22px;}
</style>


    </div>
  </div>
</div>